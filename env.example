# Environment Configuration
# Copy this file to .env and update with your values

# Application
NODE_ENV=development
VITE_APP_TITLE=sq²ft Calculators
VITE_APP_DESCRIPTION=Professional flooring and construction calculators
VITE_APP_URL=http://localhost:3000

# Analytics & Ads
VITE_ADSENSE_CLIENT=ca-pub-PLACEHOLDER
VITE_GOOGLE_ANALYTICS=GA_MEASUREMENT_ID

# PWA
VITE_PWA_NAME=sq²ft Calculators
VITE_PWA_SHORT_NAME=sq²ft
VITE_PWA_THEME_COLOR=#007bff
VITE_PWA_BACKGROUND_COLOR=#ffffff

# Performance
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE=true

# Development
VITE_DEBUG=false
VITE_MOCK_DATA=false
